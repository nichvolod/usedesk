{
  "vendor": "acceleratorusedesk",
  "name": "usedeskwidget",
  "version": "0.0.1",
  "title": "UseDesk",
  "description": "20 communication channels, all messengers, mail, calls, social networks are now just a click away",
  "builders": {
    "pixel": "0.x",
    "docs": "0.x"
  },
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true,
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x",
    "vtex.store": "2.x"
  },
  "settingsSchema": {
    "title": "UseDesk",
    "type": "object",
    "properties": {
      "domain": {
        "title": "domain",
        "description": "Domain of yours Usedesk",
        "type": "string",
        "default": "usedesk.com",
        "enum": [
          "usedesk.com",
          "usedesk.ru"
        ],
        "enumNames": [
          "usedesk.com",
          "usedesk.ru"
        ]
      },
      "widgetId": {
        "title": "Widget ID",
        "description": "Enter the Widget ID for your account in Usedesk",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
